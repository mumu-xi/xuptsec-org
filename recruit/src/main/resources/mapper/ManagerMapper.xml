<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//com.mapper.mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="org.xuptsec.recruit.dao.ManagerMapper">

    <select id="managerLogin" resultType="manager">
        select * from tb_manager where visibility=0 and username=#{username} and password=#{password}
    </select>


    <select id="findManagerTelByUsername" parameterType="java.lang.String" resultType="java.lang.String">
        select tel from tb_manager where visibility=0 and username=#{username}
    </select>

</mapper>